        <p id="timer">
          <script>
            console.log('{{horaMercado}}');
            var horaAtual = Date.now()
            var horaMercado = new Date(new Date(horaAtual).getFullYear(), new Date(horaAtual).getMonth(), new Date(horaAtual).getDate(), parseInt('{{horaMercado}}'.split(":")[0]), parseInt('{{horaMercado}}'.split(":")[1]))
            var tempoRestante = horaMercado - horaAtual
            if({{mercadoAberto}} == false) {
              document.getElementById('timer').innerHTML = "Mercado fechado"
            } else if (tempoRestante > 0) {
              setInterval(function () {
                document.getElementById('timer').innerHTML = "Tempo at√© o fechamento do mercado: " + Math.floor(tempoRestante / 1000 / 60 / 60) + ":" + Math.floor(tempoRestante / 1000 / 60 % 60) + ":" + Math.floor(tempoRestante / 1000 % 60)
                tempoRestante -= 4
              })
            } else {
              document.getElementById('timer').innerHTML = "Mercado fechado"
            }

          </script>
        </p>
